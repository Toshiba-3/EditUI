{$R *.dfm}

procedure TForm1.Button1Click(Sender: TObject);
var
  notepad: HWND;
  inputArray: array[0..3] of TInput;
begin
  notepad := FindWindow('CarmageddonClass', nil);
  SetForegroundWindow(notepad);
 end;
end.
